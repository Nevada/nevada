<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:my='urn:import:com.nevada.nevada.gwt.scaffold'>
	
	<ui:style>
		.important {
			font-weight: bold;
		}
	</ui:style>
	
	<g:TabLayoutPanel barUnit="EM" barHeight="5" ui:field="tabPanel">
		<g:tab>
			<g:header>info</g:header>
			<my:Info/>
		</g:tab>	
		<g:tab>
			<g:header>scaffold</g:header>
			<my:ScaffoldShell ui:field="shell"/>
		</g:tab>
	</g:TabLayoutPanel>
	
</ui:UiBinder> 